<% 
    renderStatus = function (status) {
        let xHtml = ''
        let color = 'success'
        let icon = 'check'

        if(status == 'inactive') {
            color = 'danger'
            icon = 'times'
        }

        xHtml = `<td class="text-center position-relative">
                    <a href="#" class="rounded-circle btn btn-sm btn-${color}">
                        <i class="fas fa-${icon}"></i>
                    </a>
                </td>`
        return xHtml
    } 
%>